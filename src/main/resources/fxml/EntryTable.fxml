<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<fx:root type="javafx.scene.control.TableView" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60">
  <columns>
    <TableColumn fx:id="nameColumn" prefWidth="75.0" text="A" />
      <TableColumn fx:id="episodeColumn" prefWidth="75.0" text="B" />
      <TableColumn fx:id="typeColumn" prefWidth="75.0" text="C" />
      <TableColumn fx:id="dateColumn" prefWidth="75.0" text="D" />
      <TableColumn fx:id="ratingColumn" prefWidth="75.0" text="E" />
  </columns>
   <columnResizePolicy>
      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
   </columnResizePolicy>
</fx:root>
